kube__redirect: Header("X-Forwarded-Proto", "http") && Weight(1000)
	-> redirectTo(302, "https:")
	-> <shunt>;

kube_foo__qux__www_example_org_____bar:
	Host("^www[.]example[.]org$") && PathRegexp("^/")
	-> <roundRobin, "http://10.2.9.103:8080", "http://10.2.9.104:8080">;

